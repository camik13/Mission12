@{
    ViewData["Title"] = "Appointment Time Slots";
}

@for (int i = 8; i < 13; i++)
{
    <form method="post">
        <input type="hidden" value="@i:00" />
        <button type="submit" class="btn btn-primary btn-lg btn-block">@i:00</button>
    </form>
}

@for (int i = 1; i < 9; i++)
{
    <form method="post">
        <input type="hidden" value="@i:00" />
        <button type="submit" class="btn btn-primary btn-lg btn-block">@i:00</button>
    </form>
}